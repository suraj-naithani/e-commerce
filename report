git init
git branch -M main
git remote add origin "url"

git add .
git commit -m "message"
git push origin branch-name

// new branch
git checkout -b new-branch-name
git add .
git commit -m "message"
git push origin branch-name

//jump from branch
git checkout branch-name

// check remote
git remote -v
// remove origin
git remote remove origin

// then again add origin
git remote add origin https://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@github.com/Surajnathani/e-commerce.git

// to pull origin code
git pull origin main